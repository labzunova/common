<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.first">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".ApplicationModified"
        android:allowBackup="true"
        android:icon="@drawable/dog"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dog"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.Light.NoActionBar">
        <activity android:name=".mainScreen.ui.MainActivity" />
        <activity android:name=".authorizationAndRegistration.RegistrationActivity" />
        <activity android:name=".otherDogProf.OtherDogActivity"/>
        <activity android:name=".matches.MatchesActivity" />
        <activity android:name=".AccountEdit.AccountEditActivity" />
        <activity android:name=".Account.AccountActivity"/>
        <activity android:name=".authorizationAndRegistration.AuthorizationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>